{
  "name": "ninja",
  "version": "1.0",
  "description": "Manage tabs with ease",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_icon": "images/tabble16.png",
    "default_popup": "index.html",
    "default_title": "ninja"
  },
  "icons": {
    "16": "/images/ninja16.png",
    "32": "/images/ninja32.png",
    "48": "/images/ninja48.png",
    "128": "/images/ninja128.png"
  },
  "manifest_version": 3
}